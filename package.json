{
  "name": "obs-utils",
  "version": "0.0.1",
  "description": "A foundry module for all your streaming needs!",
  "homepage": "https://github.com/FaeyUmbrea/obs-utils#readme",
  "bugs": {
    "url": "https://github.com/FaeyUmbrea/obs-utils/issues"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaeyUmbrea/obs-utils.git"
  },
  "type": "module",
  "license": "AGPL-3.0-only",
  "author": "Faey Umbrea",
  "main": "src/module/obs-util.js",
  "scripts": {
    "build": "vite build",
    "coverage": "nyc report",
    "instrument": "cross-env VITE_COVERAGE=\"true\" vite build",
    "lint": "eslint \"./**\" ",
    "lint:fix": "eslint \"./**\" --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:write": "prettier . --write",
    "preview": "vite preview",
    "stylelint": "stylelint \"**/*.less\"",
    "stylelint:fix": "stylelint \"**/*.less\" --fix",
    "test": "yarn playwright test --reporter=html",
    "watch": "vite build --watch",
    "dev": "vite"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.less": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@types/lodash-es": "^4.17.6",
    "lodash-es": "^4.17.21",
    "obs-websocket-js": "^5.0.2",
    "svelecte": "^3.12.0"
  },
  "devDependencies": {
    "@jhubbardsf/svelte-sortablejs": "^1.1.0",
    "@playwright/test": "^1.31.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@typhonjs-fvtt/eslint-config-foundry.js": "0.8.0",
    "@typhonjs-fvtt/runtime": "^0.0.22",
    "cross-env": "7.0.3",
    "flat": "^5.0.2",
    "less": "4.1.3",
    "nyc": "15.1.0",
    "postcss": "8.4.20",
    "postcss-less": "6.0.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "svelte": "3.55.0",
    "svelte-preprocess": "5.0.0",
    "v8-to-istanbul": "^9.0.1",
    "vite": "4.0.3",
    "vite-plugin-istanbul": "3.0.4"
  },
  "volta": {
    "node": "19.0.0",
    "yarn": "1.22.19"
  }
}
